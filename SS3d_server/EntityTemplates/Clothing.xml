<EntityTemplates>
  <EntityTemplate name="Armour">
    <Components>
      <Component name="TransformComponent"></Component>
      <Component name="VelocityComponent"></Component>
      <Component name="DirectionComponent"></Component>
      <Component name="ClickableComponent"></Component>
      <Component name="WearableAnimatedSpriteComponent">
        <Parameters>
          <Parameter name="sprite" type="string" value="player_toolbox" />
        </Parameters>
      </Component>
      <Component name="BasicInteractableComponent"></Component>
      <Component name="BasicMoverComponent"></Component>
      <Component name="BasicItemComponent"></Component>
      <Component name="PhysicsComponent">
        <Parameters>
          <Parameter name="Mass" type="float" value="5" />
        </Parameters>
      </Component>
      <Component name="EquippableComponent">
        <Parameters>
          <Parameter name="wearloc" type="string" value="Outer" />
        </Parameters>
      </Component>
      <Component name="EntityStatsComp">
        <ExtendedParameters>
          <EntityArmor type="Piercing" value="5"></EntityArmor>
          <EntityArmor type="Bludgeoning" value="5"></EntityArmor>
          <EntityArmor type="Slashing" value="5"></EntityArmor>
        </ExtendedParameters>
      </Component>
    </Components>
  </EntityTemplate>
